{% extends 'layout.html.twig' %}

{% block body %}
	{{ form_start(form)}}
		<div class="col-sm-6">
			{{ form_errors(form.name) }}
			{{ form_widget(form.name, {'attr': {'class': 'form-control'}}) }}
		</div>
		<input type="submit" class="btn btn-info" name="save" value="Save">
	{{ form_end(form) }}
{% endblock %}